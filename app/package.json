{
  "name": "sysndd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev:docker": "vite --mode docker",
    "build:docker": "vite build --mode docker",
    "build:production": "vite build --mode production",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss}\"",
    "prepare": "husky",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@unhead/vue": "^2.1.2",
    "@upsetjs/bundle": "^1.11.0",
    "@vee-validate/rules": "^4.15.1",
    "@zanmato/vue3-treeselect": "^0.4.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "bootstrap-vue-next": "^0.42.0",
    "cytoscape": "^3.33.1",
    "cytoscape-fcose": "^2.2.0",
    "cytoscape-svg": "^0.4.0",
    "d3": "^7.4.2",
    "file-saver": "^2.0.5",
    "gsap": "^3.12.1",
    "html2canvas": "^1.4.1",
    "pinia": "^2.0.14",
    "register-service-worker": "^1.7.1",
    "splitpanes": "^4.0.4",
    "swagger-ui": "^5.31.0",
    "swagger-ui-dist": "^5.31.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/cytoscape": "^3.21.9",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/compiler-sfc": "^3.5.25",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "axios": "^1.13.2",
    "cssnano": "^5.1.12",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "postcss": "^8.4.14",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.53.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix --max-warnings=50",
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --fix --max-warnings=50",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  }
}
